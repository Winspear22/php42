{
    "name": "winspear22/paquet-php42",
    "description": "Un paquet personnalisé pour installer toutes les dépendances Symfony nécessaires",
    "type": "metapackage",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "doctrine/annotations": "^2.0",
        "doctrine/dbal": "^4.2",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "symfony/form": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/validator": "6.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "repositories": {
        "paquet-php42": {
            "type": "vcs",
            "url": "https://github.com/Winspear22/paquet-php42"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "endpoint": "https://api.github.com/repos/symfony/recipes/contents/index.json",
            "docker": false
        }
    }
}
